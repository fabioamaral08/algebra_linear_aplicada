    "\n",
    "# Usando sum e multiplicação elemento a elemento\n",
    "produto_alternativo = np.sum(a * b)\n",
    "print(f\"Método alternativo: np.sum(a * b) = {produto_alternativo}\")\n",
    "\n",
    "# Usando o operador @\n",
    "produto_operador = a @ b\n",
    "print(f\"Usando operador @: a @ b = {produto_operador}\")"
