    "vetores = [\n",
    "    np.array([1, 0]),\n",
    "    np.array([0, 1]),\n",
    "    np.array([1, 1]),\n",
    "    np.array([2, -3, 1]),\n",
    "    np.array([0, 0, 0])\n",
    "]\n",
    "\n",
    "for i, vec in enumerate(vetores):\n",
    "    norma = np.linalg.norm(vec)\n",
    "    print(f\"||{vec}|| = {norma:.3f}\")"
