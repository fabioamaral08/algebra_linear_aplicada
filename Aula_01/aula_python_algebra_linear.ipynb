    "print(f\"a = {a}\")\n",
    "print(f\"b = {b}\")\n",
    "\n",
    "# Calculando o produto interno\n",
    "produto_interno = np.dot(a, b)\n",
    "print(f\"\\nProduto interno a^T b = {produto_interno}\")\n",
    "\n",
    "# Verificação manual\n",
    "produto_manual = a[0]*b[0] + a[1]*b[1] + a[2]*b[2]\n",
