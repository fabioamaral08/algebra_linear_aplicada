    "v = np.array([3, 4])\n",
    "print(f\"v = {v}\")\n",
    "\n",
    "# Usando np.linalg.norm\n",
    "norma_v = np.linalg.norm(v)\n",
    "print(f\"\\n||v|| = {norma_v}\")\n",
    "\n",
    "# Verificação manual\n",
    "norma_manual = np.sqrt(v[0]**2 + v[1]**2)\n",
